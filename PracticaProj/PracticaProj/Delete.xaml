<Window x:Class="PracticaProj.Delete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaProj"
        mc:Ignorable="d"
        Title="Delete" Height="200" Width="350">
    <UniformGrid Rows="2">
        <TextBlock Margin="0 0 0 10" HorizontalAlignment="Center" VerticalAlignment="Bottom">
            Delete row with next data name: "data name"?
        </TextBlock>
        <UniformGrid Margin="0 10 0 0" VerticalAlignment="Top" Height="30" Columns="2">
            <Button Margin="10 0">Cancel</Button>
            <Button Margin="10 0">Delete</Button>
        </UniformGrid>
    </UniformGrid>
</Window>
