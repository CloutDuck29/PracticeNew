<Window x:Class="PracticaProj.Delete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaProj"
        mc:Ignorable="d"
        Title="Delete" Height="175" Width="500">
    <Grid Style="{StaticResource MainContainer}">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Style="{StaticResource TextBlock}" Grid.Row="0" Margin="0 0 0 10" HorizontalAlignment="Center" VerticalAlignment="Bottom">
            Delete row with next data name: "data name"?
        </TextBlock>
        <UniformGrid Grid.Row="1" Margin="0 10 0 0" Columns="2">
            <Button>Cancel</Button>
            <Button>Delete</Button>
        </UniformGrid>
    </Grid>
</Window>
